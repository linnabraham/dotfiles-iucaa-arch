# multimedia keys
XF86Favorites
	mpc toggle
shift + XF86Favorites
	playerctl play-pause
XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +1000
XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -1000
XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle
XF86AudioPrev
	mpc prev
XF86AudioNext
	mpc next
XF86AudioPlay
	playerctl play-pause
XF86AudioPause
	mpc toggle
XF86MonBrightnessUp
	change-backlight.py -inc 1
	light -A 1
	xbacklight -inc 1
XF86MonBrightnessDown
	change-backlight.py -dec 1
	light -U 1
	xbacklight -dec 1
alt + XF86Favorites
	playerctl play-pause
Print
	openbox-screenshot.sh
control + Print
	maimpick
