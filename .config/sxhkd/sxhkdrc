# multimedia keys
XF86Favorites
	mpc toggle
shift + XF86Favorites
	playerctl play-pause
XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +1000
XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -1000
XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle
XF86AudioPrev
	mpc prev
XF86AudioNext
	mpc next
XF86AudioPlay
	playerctl play-pause
XF86AudioPause
	mpc toggle
XF86MonBrightnessUp
	change-backlight.py -inc 1
	light -A 1
	xbacklight -inc 1
XF86MonBrightnessDown
	change-backlight.py -dec 1
	light -U 1
	xbacklight -dec 1
alt + XF86Favorites
	playerctl play-pause
super + Return
	xfce4-terminal
	gnome-terminal
	kitty
	zsh -i -c "tmux_with_terminal"
	alacritty
super + a
	j4-dmenu-desktop
#	jgmenu_run
super + A
	dmenu_run
super + BackSpace
	screenkey
super + b
	bukumenu
	dmenu-search-urls.sh
super + B
	bukuadd
# key binding used up by autokey for inserting date
super + D
	rofi-date	
	#rofi -i -show run
#	buku-dmenu
super + s
	rofi-screenshot
super + S
	rofi-screenshot -s
super + u
	wrapper-wmctrl.sh mpdevil
	jumpapp mpdevil
	jumpapp ymuse
super + w
	switch-i3-workspace.sh
#	rofi-dict.sh
super + slash
	mpc_control -t
super + question
	dmenu-wrapper-music
#	hotkey-helper.sh
shift + Prior
	mpc prev
shift + Next
	mpc next
Print
	openbox-screenshot.sh
control + Print
	maimpick
